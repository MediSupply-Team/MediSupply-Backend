[run]
source = app
omit =
    # Scripts de inicialización y población (no son lógica de negocio)
    app/populate_db.py
    app/create_catalogo_db.py
    app/db_init.py
    
    # Workers que se ejecutan por separado
    app/worker/*
    
    # Servicios externos que requieren AWS/SQS configurado
    app/services/aws_service.py
    app/services/task_service.py
    
    # Tests
    tests/*
    
[report]
exclude_lines =
    # Líneas estándar para excluir
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

